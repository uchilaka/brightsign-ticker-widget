{
  "name": "brightsign-ticker",
  "version": "1.0.0",
  "description": "Ticker to show stock price in brightsign display",
  "main": "index.js",
  "license": "UNLICENSED",
  "private": false,
  "scripts": {
    "simulate-0": "json-server --watch ./build/ticker_data.json --port ${PORT:-8080}",
    "start-0": "json-server -H 0.0.0.0 ./build/ticker_data.json --port $PORT",
    "simulate": "node-dev demo.js",
    "start": "node demo.js"
  },
  "dependencies": {
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "json-server": "^0.14.0",
    "request": "^2.88.0",
    "request-promise": "^4.2.2"
  },
  "engines": {
    "node": "~8.11"
  }
}
