{
  "name": "brightsign-ticker",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "json-server": "^0.14.0",
    "request": "^2.88.0",
    "request-promise": "^4.2.2",
    "react": "^16.5.1",
    "react-dom": "^16.5.1",
    "react-scripts": "1.1.5"
  },
  "scripts": {
    "simulate-0": "json-server --watch ./build/ticker_data.json --port ${PORT:-8080}",
